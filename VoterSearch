<script src="//ajax.googleapis.com/ajax/libs/jquery/1.8/jquery.min.js" type="text/javascript"></script>
    <script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.8.23/jquery-ui.min.js" type="text/javascript"></script>
    <script type="text/javascript" src="../Scripts/jquery.autotab-1.1b.js"></script>

 <script type="text/javascript">
     //http://www.mathachew.com/sandbox/jquery-autotab/

     $(document).ready(function () {
             //ACCORDIAN CONTROL
             //any item with the id of "accordian" will call the accordian functions in the jquery UI.
             $("#accordion").accordion({ active: false, animate: false, autoHeight: false, collapsible: true });

         $('#txtDOBMonth').autotab({ target: 'txtDOBDay', format: 'numeric' });
         $('#txtDOBDay').autotab({ target: 'txtDOBYear', format: 'numeric', previous: 'txtDOBMonth' });
//         $('#txtDOBYear').autotab({ target: 'txtSearchZipCode', format: 'numeric', previous: 'txtDOBDay' });

        //remove illegal characters
            $(document).ready(function () {
                $("#txtSearchFirstName").blur(function () {
                    var input = $("#txtSearchFirstName").val();
                    input = input.replace(/[^A-Za-z\s]/g, '');
                    $("#txtSearchFirstName").val(input);
                });

                $("#txtSearchLastName").blur(function () {
                    var input = $("#txtSearchLastName").val();
                    input = input.replace(/[^A-Za-z\s\-]/g, '');
                    $("#txtSearchLastName").val(input);
                });

                $("#txtSearchZipCode").blur(function () {
                    var input = $("#txtSearchZipCode").val();
                    input = input.replace(/[^0-9]/g, '');
                    $("#txtSearchZipCode").val(input);
                });

                $("#txtSearchHouseNumber").blur(function () {
                    var input = $("#txtSearchHouseNumber").val();
                    input = input.replace(/[^0-9]/g, '');
                    $("#txtSearchHouseNumber").val(input);
                });

                $("#txtSearchMiddleInitial").blur(function () {
                    var input = $("#txtSearchMiddleInitial").val();
                    input = input.replace(/[^A-Za-z]/g, '');
                    $("#txtSearchMiddleInitial").val(input);
                });
            });
        
     });       
    </script>


    <!--google analytics script-->
 <script type="text/javascript">

     var _gaq = _gaq || [];
     _gaq.push(['_setAccount', 'UA-32945568-1']);
